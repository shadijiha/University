{
    "name": "shado-script",
    "displayName": "Shado-Language-support",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "shado",
            "aliases": [
                "Shado",
                "shado"
            ],
            "extensions": [
                ".sscript",
                ".ss"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "shado",
            "scopeName": "source.shado",
            "path": "./syntaxes/shado.tmLanguage.json"
        }]
    }
}